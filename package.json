{
  "name": "slackie",
  "version": "0.0.1",
  "description": "Simple frontend slack clone",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "prod": "webpack -p --define process.env.NODE_ENV='\"production\"' --progress --colors --config webpack.config.production.js",
